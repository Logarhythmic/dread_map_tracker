{
    "shared_item_grid": {
        "type": "array",
        "orientation": "vertical",
        "margin": "0,0",
        "content": [
            {
                "type": "array",
                "orientation": "horizontal",
                "margin": "0,0",
                "content": [
                    {
                        "type": "itemgrid",
                        "item_margin": "6,1",
                        "h_alignment": "left",
                        "rows": [
                            [ "wide"    , "charge" , "grapple" , "morph", "bomb" , "pb"    ],
                            [ "anymissile" ,  "storm" ,  "spider" , "speed"   , "anyjump" , "screw" ],
                            [ "anysuit"    ,  "cloak" ,  "flash"  , "radar"   ,  "etank"  , "epart" ]
                        ]
                    },
                    {
                        "type": "array",
                        "orientation": "vertial",
                        "v_alignment": "right",
                        "margin": "0,0,0,0",
                        "content": [
                            {
                                "type": "itemgrid",
                                "margin": "0,0",
                                "item_margin": "6,1",
                                "h_alignment": "left",
                                "rows": [
                                    [ "white",  "green" ,  "yellow"       ],
                                    [ "blue" ,  "purple",   "red"         ],
                                    [   "qr" ,     "x"  ,   "go"          ]
                                ]
                            }
                        ]
                    }
                ]
            }
        ]
    },
        "tracker_capture_item": {
        "type": "container",
        "content": {
            "type": "itemgrid",
            "item_margin": "6,1",
            "h_alignment": "left",
            "rows": [
                [ "charge", "wide",  "super",  "storm", "grapple" ],
                [ "varia",  "morph", "bomb",   "cross", "pb"      ],
                [ "slide",  "space", "spider", "speed", "screw"   ],
                [ "cloak",  "flash", "radar",  "etank", "epart"   ]
            ]
        }
    },
